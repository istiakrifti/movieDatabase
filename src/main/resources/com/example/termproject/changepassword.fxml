<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.termproject.ChangepasswordControler">
   <children>
      <Button layoutX="215.0" layoutY="252.0" mnemonicParsing="false" onAction="#onChangeClick" prefHeight="26.0" prefWidth="68.0" text="Change ">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Label fx:id="label" layoutX="158.0" layoutY="52.0" prefHeight="26.0" prefWidth="195.0" />
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#onBackClick" text="Back" />
      <PasswordField fx:id="currentpass" layoutX="158.0" layoutY="106.0" prefHeight="26.0" prefWidth="206.0" promptText="Current Password" />
      <PasswordField fx:id="newpass" layoutX="158.0" layoutY="152.0" prefHeight="26.0" prefWidth="206.0" promptText="New Password" />
      <PasswordField fx:id="newpass2" layoutX="158.0" layoutY="200.0" prefHeight="26.0" prefWidth="206.0" promptText="Confirm New Password" />
   </children>
</AnchorPane>
